<?xml version="1.0" encoding="UTF-8"?>

<Shell
    x:Class="Haihv.Elis.Tools.App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Haihv.Elis.Tools.App"
    Title="Trích xuất dữ liệu Elis">

    <ShellContent
        Title="Trang chủ"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Trích xuất dữ liệu"
        ContentTemplate="{DataTemplate local:ExportDataToXml}"
        Route="ExportData" />

</Shell>